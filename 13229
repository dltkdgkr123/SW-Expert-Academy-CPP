#include <iostream>

int main()
{
	std::string s[] = {"MON", "TUE", "WED", "THU", "FRI", "SAT", "SUN"};


	int t;
	std::cin >> t;

	for (int i = 0;i < t;i++)
	{
		std::string input;
		std::cin >> input;

		if (input == "SUN")
		{
			std::cout << "#" << i + 1 << " " << 7 << "\n";
			continue;
		}

		for (int j = 0;j < 7;j++)
		{
			if (input == s[j])
			{
				std::cout << "#" << i + 1 << " " << 6 - j << "\n";
				break;
			}
		}
	}
}
